<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    
    <!--#region Gradient Color-->
    <LinearGradientBrush x:Key="cs_Background" StartPoint="0,0" EndPoint="0,5">
        <GradientStop Color="#EBEEF4" Offset="0.5"/>
    </LinearGradientBrush>
    <!--#endregion-->

    <!--#region Solid Color-->
    <SolidColorBrush x:Key="cs_BorderBrush" Color="Gray"/>

    <SolidColorBrush x:Key="cs_ImgAct" Color="Black"/>

    <SolidColorBrush x:Key="cs_ImgPas" Color="Black"/>

    <SolidColorBrush x:Key="cs_Txt" Color="Black"/>
    <!--#endregion-->

</ResourceDictionary>